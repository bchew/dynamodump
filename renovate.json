{
  "additionalReviewers": ["bchew"],
  "branchPrefix": "renovate-",
  "extends": ["config:base"],
  "labels": ["dependencies"],
  "packageRules": [
    {
      "matchPackageNames": ["boto3"],
      "schedule": ["on the first day of the month"]
    }
