{
  "additionalReviewers": ["bchew"],
  "branchPrefix": "renovate-",
  "extends": ["config:base", "schedule:monthly"],
  "labels": ["dependencies"],
  "packageRules": [
    {
      "matchPackageNames": ["boto3"],
      "schedule": ["every 3 months on the first day of the month"]
    }
  ],
  "pip_requirements": {
    "fileMatch": ["^requirements.*\\.txt"]
  },
  "pre-commit": {
    "enabled": true
  }
}
